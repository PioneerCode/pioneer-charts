{
  "name": "pioneer-charts",
  "version": "1.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve pioneer-charts-dev",
    "start:docs": "ng serve pioneer-charts --port 4201",
    "build": "ng build",
    "build:docs": "ng build pioneer-charts --prod",
    "build:docs:404": "node ./build/404.js",
    "build:docs:prod": "npm run build:lib && npm run build:docs && npm run build:docs:404",
    "build:lib": "npm run build:lib:package && npm run build:lib:sass && npm run build:lib:theme  && npm run build:lib:readme",
    "build:lib:publish": "npm run build:lib && npm publish dist/pcac --access public",
    "build:lib:publish:tag": "npm run build:lib && npm publish dist/pcac --access public --tag rc",
    "build:lib:readme": "node ./build/readme.js",
    "build:lib:package": "ng build pcac",
    "build:lib:theme": "node ./build/theme.js",
    "build:lib:sass": "node ./build/sass.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/forms": "~8.0.1",
    "@angular/platform-browser": "~8.0.1",
    "@angular/platform-browser-dynamic": "~8.0.1",
    "@angular/router": "~8.0.1",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.3",
    "@angular-devkit/build-ng-packagr": "~0.800.3",
    "@angular/cli": "~8.0.3",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@pioneer-code/pioneer-charts": "^1.0.1",
    "@ryancavanaugh/prismjs": "^1.4.3-alpha",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bootstrap": "^4.3.1",
    "chalk": "^2.4.2",
    "codelyzer": "^5.0.0",
    "copyfiles": "^2.1.0",
    "cpy": "^7.3.0",
    "d3": "^5.9.2",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "node-sass": "^4.13.1",
    "prismjs": "^1.16.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.3"
  }
}
