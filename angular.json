{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pioneer-charts-web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/pioneer-charts-web",
      "sourceRoot": "projects/pioneer-charts-web/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "dist/pioneer-charts-web"
            },
            "index": "projects/pioneer-charts-web/src/index.html",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/pioneer-charts-web/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/pioneer-charts-web/src/favicon.ico",
              "projects/pioneer-charts-web/src/assets"
            ],
            "styles": [
              "projects/pioneer-charts-web/src/styles.scss"
            ],
            "scripts": [],
            "browser": "projects/pioneer-charts-web/src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "pioneer-charts-web:build:production"
            },
            "development": {
              "buildTarget": "pioneer-charts-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "pioneer-charts-web:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/pioneer-charts-web/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/pioneer-charts-web/src/favicon.ico",
              "projects/pioneer-charts-web/src/assets"
            ],
            "styles": [
              "projects/pioneer-charts-web/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "pioneer-charts-dev": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/pioneer-charts-dev",
      "sourceRoot": "projects/pioneer-charts-dev/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "dist/pioneer-charts-dev"
            },
            "index": "projects/pioneer-charts-dev/src/index.html",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/pioneer-charts-dev/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/pioneer-charts-dev/src/favicon.ico",
              "projects/pioneer-charts-dev/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "projects/pioneer-charts-dev/src/styles.scss"
            ],
            "scripts": [],
            "browser": "projects/pioneer-charts-dev/src/main.ts",
                     "stylePreprocessorOptions": {
              "sass": {
                "silenceDeprecations": [
                  "mixed-decls",
                  "color-functions",
                  "global-builtin",
                  "import"
                ]
              }
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "pioneer-charts-dev:build:production"
            },
            "development": {
              "buildTarget": "pioneer-charts-dev:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "pioneer-charts-dev:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/pioneer-charts-dev/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/pioneer-charts-dev/src/favicon.ico",
              "projects/pioneer-charts-dev/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "projects/pioneer-charts-dev/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "@pioneer-code/pioneer-charts": {
      "projectType": "library",
      "root": "projects/pioneer-charts",
      "sourceRoot": "projects/pioneer-charts/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/pioneer-charts/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/pioneer-charts/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/pioneer-charts/tsconfig.spec.json"
          }
        }
      }
    },
    "pioneer-charts-docs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/pioneer-charts-docs",
      "sourceRoot": "projects/pioneer-charts-docs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/pioneer-charts-docs/src/main.ts",
            "tsConfig": "projects/pioneer-charts-docs/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/pioneer-charts-docs/public"
              }
            ],
            "styles": [
              "node_modules/modern-normalize/modern-normalize.css",
              "projects/pioneer-charts-docs/src/styles.scss"
            ],
            "server": "projects/pioneer-charts-docs/src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "projects/pioneer-charts-docs/src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "pioneer-charts-docs:build:production"
            },
            "development": {
              "buildTarget": "pioneer-charts-docs:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/pioneer-charts-docs/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/pioneer-charts-docs/public"
              }
            ],
            "styles": [
              "projects/pioneer-charts-docs/src/styles.scss"
            ]
          }
        }
      }
    }
  }
}